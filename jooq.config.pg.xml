<!-- generate from PostgreSQL -->
<configuration>
    <jdbc>
        <driver>org.postgresql.Driver</driver>
        <url>jdbc:postgresql://localhost:5432/pg-dev</url>
        <user>postgres</user>
        <password>sa</password>
    </jdbc>
    <generator>
        <database>
            <name>org.jooq.meta.postgres.PostgresDatabase</name>
            <includes>.*</includes>
            <excludes>payment_p2007_0.* | Flyway.*</excludes>
            <inputSchema>public</inputSchema>
        </database>
        <target>
            <packageName>org.brum.sjs.jooq.domain</packageName>
            <directory>target/generated/jooq</directory>
        </target>
        <generate>
            <relations>true</relations>
            <pojos>true</pojos>
            <records>true</records>
            <interfaces>true</interfaces>
            <daos>true</daos>
            <fluentSetters>true</fluentSetters>
            <indexes>true</indexes>
            <immutablePojos>true</immutablePojos>
            <deprecationOnUnknownTypes>false</deprecationOnUnknownTypes>
            <generatedAnnotation>true</generatedAnnotation>
            <jpaAnnotations>true</jpaAnnotations>
            <validationAnnotations>true</validationAnnotations>
        </generate>
    </generator>
</configuration>